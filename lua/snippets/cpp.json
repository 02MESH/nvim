  "ListNode": {
    "prefix": "ListNode",
    "description": "Definition of a ListNode structure",
    "body": [
      "struct ListNode {",
      "\tint val;",
      "\tListNode *next;",
      "\tListNode() : val(0), next(nullptr) {}",
      "\tListNode(int x) : val(x), next(nullptr) {}",
      "\tListNode(int x, ListNode *next) : val(x), next(next) {}",
      "};"
    ]
